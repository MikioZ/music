{
  "name": "vite-boot",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.4.0",
  "description": "Vite starter template with UnoCSS",
  "keywords": [
    "viteBoot",
    "vite-boot",
    "vite",
    "unocss",
    "fast",
    "boot"
  ],
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@kirklin/logger": "^0.0.2",
    "@kirklin/reset-css": "^0.0.3",
    "@vueuse/core": "^10.1.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.35",
    "unocss-preset-chinese": "^0.0.6",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.50",
    "@iconify-json/tabler": "^1.1.74",
    "@kirklin/eslint-config": "^0.3.4",
    "@types/node": "^20.0.0",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^4.2.1",
    "eslint": "^8.39.0",
    "pnpm": "^8.4.0",
    "typescript": "^5.0.4",
    "unocss": "^0.51.8",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.4",
    "vite-plugin-inspect": "^0.7.25",
    "vite-plugin-vue-inspector": "^3.4.1",
    "vue-tsc": "^1.6.4"
  },
  "lint-staged": {
    "**/*": "eslint --fix"
  }
}
